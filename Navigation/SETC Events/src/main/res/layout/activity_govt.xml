                    <ImageView
                        android:id="@+id/im9"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="20dp"
                        android:adjustViewBounds="false"
                        android:background="@drawable/border_shape"
